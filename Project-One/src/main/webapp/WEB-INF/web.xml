<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>submit</servlet-name>
		<servlet-class>com.revature.servlets.SubmitServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>request</servlet-name>
		<servlet-class>com.revature.servlets.RequestServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>view</servlet-name>
		<servlet-class>com.revature.servlets.ViewServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>requestAll</servlet-name>
		<servlet-class>com.revature.servlets.RequestAllServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>submit</servlet-name>
		<url-pattern>/submit</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>request</servlet-name>
		<url-pattern>/request</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>requestAll</servlet-name>
	<url-pattern>/requestAll</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>view</servlet-name>
	<url-pattern>/view</url-pattern>
	</servlet-mapping>


</web-app>
